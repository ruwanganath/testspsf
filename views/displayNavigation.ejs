<!DOCTYPE HTML>
<html lang="en">

<head>
  <%- include("partials/head",title) %>

  <script type='text/javascript'>

    var select_lat = "<%= slat %>";
    var select_lon = "<%= slon %>";
    
  </script>

</head>
<body>
  <header>
    <%- include ("partials/header",loggedIn,signIn) %>
  </header>
  <div class="loggeduser">&nbsp;&nbsp;Welcome&nbsp;<%= username %></div>
  <main>
        <%- include ("partials/navigation",timer,bay,username,mobileno,slat,slon) %>
  </main>
  <footer>
    <%- include ("partials/footer") %>
  </footer>
  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <script async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDIcnRNMgtkn-k-x9wGwCb8GeMvOLUg2jA&callback=initMap"></script>

  <script src="nav.js"></script>
  <script type="text/javascript">
    //Perodically update the map and check the bay availability
    setInterval(function(){
      initMap();
      navigate_to_location(select_lat,select_lon);
    
      getUpdatedAvailableParkingData();

      if(!isAvailable)
      {
        alert("Parking Bay is not available");
      }

      isAvailable=false;

      

    },120000);  
  
  </script>
  <script src="nav.js"></script>
  <script src="timepicker.js"></script>
  <script async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCrjMOa0vDaQWFf3MkA5K-Ttr6S3lCAGp8&callback=initMap"></script>  
</body>

</html>
